<% if (page.total > 1){ %>
<nav class="pagination" role="navigation">
    <div id="page-nav">
        <%- paginator({
            escape: false,
            prev_text: "<i class='fa fa-angle-left'></i>",
            next_text: "<i class='fa fa-angle-right'></i>"
        }) %>
    </div>
</nav>
<% } %>